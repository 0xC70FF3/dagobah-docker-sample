{
    "name": "My First Awesome Job Queue",
    "tasks": [{
        "name": "Do Something",
        "command": "echo \"Then do something\""
    }, {
        "name": "Wait a little...",
        "command": "sleep 2"
    }, {
        "name": "Say Hello",
        "command": "echo \"Hello World!\""
    }],
    "dependencies": {
        "Do Something": ["Wait a little..."],
        "Wait a little...": ["Say Hello"],
        "Say Hello": []
    }
}
